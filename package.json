{
  "name": "thought_tagger",
  "version": "1.0.0",
  "description": "This is a front-end only, web-app for segmenting audio-recordings into separate *thoughts*. It's built using [Svelte](https://svelte.dev/), [Bulma](https://bulma.io/documentation/), [Peaks.js](https://github.com/bbc/peaks.js), and uses [Firebase](https://firebase.google.com/) to store data.",
  "main": ".eslintrc.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "npm run build && npm run start",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@kazzkiq/svelte-preprocess-scss": "^0.1.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "bulma": "^0.9.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.56.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.38.1",
    "svelte": "^3.42.3",
    "svelte-preprocess-sass": "^2.0.1"
  },
  "dependencies": {
    "firebase": "^7.24.0",
    "peaks.js": "^0.17.0",
    "sirv-cli": "^1.0.14"
  }
}
